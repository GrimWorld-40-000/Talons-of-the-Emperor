            <fixedParams>
                <filter>
                    <thingDefs>
                        <li>AdvancedMedkit</li>
                    </thingDefs>
                </filter>
            </fixedParams>
        </root>
    </ThingSetMakerDef>

    <FactionDef ParentName="GW_ImperialFactionBase">
