                    <GW_TotE_CustodesFabricationFabricationGuard>3</GW_TotE_CustodesGuard>
                    <GW_TotE_CustodesWarden>3</GW_TotE_CustodesWarden>
                    <GW_TotE_Allarus>1</GW_TotE_Allarus>
                    <GW_TotE_Castellan>1</GW_TotE_Castellan>
                </options>
            </li>
        </pawnGroupMakers>
