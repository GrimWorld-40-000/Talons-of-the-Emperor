        <configurationListOrderPriority>998</configurationListOrderPriority>
        <leaderTitle>lord general</leaderTitle>
        <techLevel>Ultra</techLevel>
        <pawnSingular>imperial</pawnSingular>
        <pawnsPlural>imperials</pawnsPlural>
