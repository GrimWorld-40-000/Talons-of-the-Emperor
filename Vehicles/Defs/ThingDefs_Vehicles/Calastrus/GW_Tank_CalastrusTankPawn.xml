<?xml version="1.0" encoding="utf-8"?>
<Defs>

<!--===== Leman Russ =====-->
	
	<Vehicles.VehicleDef ParentName="BaseVehiclePawn">
		<defName>GW_CalastrusTank</defName>
		<label>Calastrus</label>
		<description>The Calastrus, WIP. \n\n&lt;color=#bb8f04&gt;Crew:&lt;/color&gt; Driver x1, Gunner x2, Passenger x12\n&lt;color=#bb8f04&gt;Fuel type:&lt;/color&gt; Uranium</description>
		<graphicData>
			<texPath>Things/Vehicles/Calastrus/Calastrus</texPath>
			<graphicClass>Vehicles.Graphic_Vehicle</graphicClass>
			<shaderType>CutoutComplexPattern</shaderType>
			<drawSize>(13, 14)</drawSize>
			<color>(155, 155, 155)</color>
			<colorTwo>(65, 75, 80)</colorTwo>
			<colorThree>(45, 50, 45)</colorThree>
		</graphicData>
		<fillPercent>1</fillPercent>
		<buildDef>GW_CalastrusTank_Blueprint</buildDef>
		<size>(9, 11)</size>
		<vehicleMovementPermissions>DriverNeeded</vehicleMovementPermissions>
		<vehicleCategory>Combat</vehicleCategory>
		<techLevel>Spacer</techLevel>
		<vehicleType>Land</vehicleType>
		<statBases>
			<ArmorRating_Blunt>1.2</ArmorRating_Blunt>
			<ArmorRating_Sharp>2</ArmorRating_Sharp>
			<ArmorRating_Heat>0.4</ArmorRating_Heat>
			<Flammability>0</Flammability>
			<MarketValue>15000</MarketValue>
		</statBases>
		<vehicleStats>
			<Mass>590</Mass>
			<MoveSpeed>7.4</MoveSpeed>
			<CargoCapacity>1000</CargoCapacity>
			<RepairRate>0.3</RepairRate>
			<AccelerationRate MayRequire="OskarPotocki.VanillaVehiclesExpanded">0.04</AccelerationRate>
		</vehicleStats>
		
		<soundSustainersOnEvent>
			<li>
				<start>IgnitionOn</start>
				<stop>IgnitionOff</stop>
				<value>GW_VoG_HeavySound</value>
			</li>
		</soundSustainersOnEvent>
		
		<race>
			<baseBodySize>4.0</baseBodySize>
		</race>
		<properties>
			<customSnowCategoryTicks AssignDefaults="1" />
			<customRoadCosts AssignDefaults="0.33" />
			<winterSpeedMultiplier>0.75</winterSpeedMultiplier>
			<worldSpeedMultiplier>1.25</worldSpeedMultiplier>
			<offRoadMultiplier>0.75</offRoadMultiplier>
			<customTerrainCosts AssignDefaults = "1"/>
		<iconTexPath>UI/Icons/DefaultBoatIcon</iconTexPath>
			<customBiomeCosts>
				<li>
					<key>Ocean</key>
					<value>0.3</value>
				</li>
				<li>
					<key>Lake</key>
					<value>0.3</value>
				</li>
			</customBiomeCosts>
			<vehicleJobLimitations>
				<li>(RepairVehicle, 5)</li>
			</vehicleJobLimitations>
			<roles>
				<li>
					<key>driver</key>
					<label>Driver</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					<handlingTypes>
						<li>Movement</li>
						<li>Turret</li>
					</handlingTypes>
					<turretIds>
						<li>frontMountedTurret</li>
					</turretIds>
				</li>
				<li>
					<key>gunnerone</key>
					<label>Cannon Operator</label>
					<slots>1</slots>
					<slotsToOperate>1</slotsToOperate>
					<handlingTypes>Turret</handlingTypes>
					<turretIds>
						<li>mainTurret</li>
					</turretIds>
				</li>
			</roles>
		</properties>
		<components>
			<li>
				<key>Engine</key>
				<label>Engine</label>
				<health>150</health>
				<priorityStatEfficiency>true</priorityStatEfficiency>
				<depth>Internal</depth>
				<reactors>
					<li Class="Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<radius>3</radius>
						<damageDef>Bomb</damageDef>
					</li>
				</reactors>
				<hitbox>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>AmmoRack</key>
				<label>Ammo Rack</label>
				<health>50</health>
				<depth>Internal</depth>
				<reactors>
					<li Class="Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.25</chance>
						<radius>2</radius>
						<damageDef>Bomb</damageDef>
					</li>
				</reactors>
			</li>
			<li>
				<key>LeftTrack</key>
				<label>Left Hover Rack</label>
				<health>300</health>
				<depth>External</depth>
				<tags>
					<li>Wheel</li>
				</tags>
				<hitbox>
					<fallthrough>false</fallthrough>
					<side>Left</side>
				</hitbox>
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>RightTrack</key>
				<label>Right Hover Rack</label>
				<health>200</health>
				<depth>External</depth>
				<tags>
					<li>Wheel</li>
				</tags>
				<hitbox>
					<fallthrough>false</fallthrough>
					<side>Right</side>
				</hitbox>
				<categories>
					<li>MoveSpeed</li>
				</categories>
			</li>
			<li>
				<key>FrontArmorPlating</key>
				<label>Front Armor Plating</label>
				<health>300</health>
				<depth>External</depth>
				<armor>
					<ArmorRating_Blunt>1.72</ArmorRating_Blunt>
					<ArmorRating_Sharp>3.04</ArmorRating_Sharp>
				</armor>
				<hitbox>
					<side>Front</side>
				</hitbox>
			</li>
			<li>
				<key>LeftArmorPlating</key>
				<label>Left Armor Plating</label>
				<health>255</health>
				<depth>External</depth>
				<armor>
					<ArmorRating_Blunt>1.17</ArmorRating_Blunt>
					<ArmorRating_Sharp>2.07</ArmorRating_Sharp>
				</armor>
				<hitbox>
					<side>Left</side>
				</hitbox>
			</li>
			<li>
				<key>RightArmorPlating</key>
				<label>Right Armor Plating</label>
				<health>255</health>
				<depth>External</depth>
				<armor>
					<ArmorRating_Blunt>1.17</ArmorRating_Blunt>
					<ArmorRating_Sharp>2.07</ArmorRating_Sharp>
				</armor>
				<hitbox>
					<side>Right</side>
				</hitbox>
			</li>
			<li>
				<key>BackArmorPlating</key>
				<label>Rear Armor Plating</label>
				<health>205</health>
				<depth>External</depth>
				<armor>
					<ArmorRating_Blunt>0.94</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.66</ArmorRating_Sharp>
				</armor>
				<hitbox>
					<side>Back</side>
				</hitbox>
			</li>
			<li>
				<key>FuelTank</key>
				<label>Fuel Tank</label>
				<health>125</health>
				<depth>Internal</depth>
				<reactors>
					<li Class="Vehicles.Reactor_FuelLeak" />
					<li Class="Vehicles.Reactor_Explosive">
						<indicator>VF_Explosive</indicator>
						<chance>0.15</chance>
						<radius>5</radius>
						<damageDef>Bomb</damageDef>
					</li>
				</reactors>
				<hitbox>
					<cells>
						<li>(0, -1)</li>
					</cells>
				</hitbox>
			</li>
			<li>
				<key>Dashboard</key>
