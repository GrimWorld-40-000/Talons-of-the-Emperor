				<li>
					<key>Ocean</key>
					<value>0.3</value>
				</li>
				<li>
					<key>Lake</key>
					<value>0.3</value>
				</li>
			</customBiomeCosts>
			<vehicleJobLimitations>
				<li>(RepairVehicle, 5)</li>
			</vehicleJobLimitations>
			<diagonalRotation>false</diagonalRotation>
			<roles>
				<li>
